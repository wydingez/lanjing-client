
{
  "compilerOptions": {
    "target": "ES6",
    "jsx": "react-native",
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "_c/*": ["src/components/*"],
      "_v/*": ["src/views/*"],
      "static/*": ["public/static/*"],
      "_d/*": ["src/data/*"],
    }
  },
  "exclude": ["node_modules"]
}  